[ignore]
<PROJECT_ROOT>/node_modules/.*/node_modules/fbjs/.*
<PROJECT_ROOT>/config-overrides.js

[include]

[libs]
flow-typed/

[lints]

[options]
module.system=haste

module.file_ext=.js

module.name_mapper='^actions' -> '<PROJECT_ROOT>/src/actions'
module.name_mapper='^reducers' -> '<PROJECT_ROOT>/src/reducers'
module.name_mapper='^components' -> '<PROJECT_ROOT>/src/components'
module.name_mapper='^store' -> '<PROJECT_ROOT>/src/store'
module.name_mapper='.*\(.css\)' -> 'CSSModule'

[strict]
